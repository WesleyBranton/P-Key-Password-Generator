{

  "manifest_version": 2,
  "name": "Password Generator",
  "version": "3.1",

  "description": "Generate a strong password to protect your online accounts.",

  "icons": {
    "48": "icons/password-48.png",
	"96": "icons/password-96.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/password-32.png",
    "default_title": "Password Generator",
    "default_popup": "popup/generator.html"
  }

}